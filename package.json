{
  "name": "large-reader",
  "version": "0.0.21-alpha",
  "description": "Generate a website for an NFT collection built with Large.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LargeNFT/large-reader/"
  },
  "scripts": {
    "serve": "node ./node_modules/http-server/bin/http-server -p 8081 -c-1",
    "generate": "node node_modules/large-nft/public/large/generate/index.js",
    "generate:dev": "node node_modules/large-nft/public/large/generate/index.js --env dev",
    "sync": "node --no-experimental-fetch node_modules/large-nft/public/large/sync/index.js",
    "start": "node --no-experimental-fetch node_modules/large-nft/public/large/start.js",
    "publish": "node --no-experimental-fetch node_modules/large-nft/public/large/admin/publish.js"
  },
  "keywords": [],
  "author": "Patrick Toner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LargeNFT/large-reader/issues"
  },
  "type": "module",
  "devDependencies": {

    "http-server": "14.1.1",
    "fastify": "4.12.0",
    "@fastify/static": "6.8.0",
    "convert-svg-to-png": "0.6.4",
    "sharp": "0.31.3",
    "pouchdb-node": "8.0.1",
    "ipfs-http-client": "60.0.0",

    "large-nft": "^0.16.0"
  }
}
